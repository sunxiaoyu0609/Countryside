Page {
  font-size: 14px;
}

.top {
  width: 100%;
  height: 180px;
  background: #0E9489;
  padding-top: 15px;
  position: relative;
}

.center {
  width: 95%;
  height: 150px;
  background: white;
  display: flex;
  flex-direction: column;
  margin: 0 auto;
  border-radius: 5px;
}

.center_top {
  display: flex;
  flex-direction: column;
  width: 80%;
  height: 80px;
  margin: 0 auto;
  margin-top: 20rpx;
  border-bottom: 1px solid #EEEEEE;

}
.manager{
  margin: 0 auto;
  justify-content: start;
  align-content: center;
  margin-top: 25px;
  font-size: 20px;
}
.welcome{
  margin: 0 auto;
  font-size: 10px;
  justify-content: start;
  align-content: center;
}

.bottom{
  width: 95%;
  height: 105px;
  background: white;
  display: flex;
  flex-direction: column;
  margin: 0 auto;
  border-radius: 5px;
  box-shadow: 5rpx 5rpx 5rpx 5rpx #0E9489;
  margin-top: 20px;
}
.text{
  margin: -1px 0 7px 113px;
  font-size: small;
  color: red;
}
.btn{
  display: flex;
  flex-direction: column;
}
.outlogin,.changepwd{
  width: 90%;
  margin: 10px auto;
  display: block;
}



